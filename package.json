{
    "name": "mock_premier_league",
    "version": "1.0.0",
    "description": "Awonugba Seun Software Developer Application Test Solution (Mock Premier League)",
    "main": "app.js",
    "scripts": {
        "build": "rimraf dist && npx tsc",
        "prestart": "npm run build",
        "start": "cross-env NODE_ENV=production node dist/app.js",
        "test": "cross-env NODE_ENV=test jest",
        "predev": "npm run build",
        "dev": "cross-env NODE_ENV=development concurrently \"npx tsc -w\" \"nodemon dist/app.js\"",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
        "db:refresh:seed": "fresh migrate seed",
        "db:fresh:seed": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
        "db:seed:all": "npx sequelize-cli db:seed:all",
        "test:admin": "npm test -- admin.test.ts",
        "test:team": "npm test -- team.test.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/seunAwonugba/Mock-Premier-League.git"
    },
    "author": "Awonugba Seun",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/seunAwonugba/Mock-Premier-League/issues"
    },
    "homepage": "https://github.com/seunAwonugba/Mock-Premier-League#readme",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "connect-redis": "^7.1.1",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "csv-parser": "^3.0.0",
        "dotenv": "^16.4.5",
        "express": "^4.21.0",
        "express-rate-limit": "^7.4.0",
        "express-session": "^1.18.0",
        "http-status-codes": "^2.3.0",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "nanoid": "^3.0.0",
        "pg": "^8.13.0",
        "pg-hstore": "^2.3.4",
        "redis": "^4.7.0",
        "rimraf": "^6.0.1",
        "sequelize": "^6.37.3",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.0",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^22.7.4",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^10.0.0",
        "concurrently": "^9.0.1",
        "jest": "^29.7.0",
        "nodemon": "^3.1.7",
        "sequelize-cli": "^6.6.2",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.2"
    }
}
